{
    "parameters": [
      { "name": "thickness", "value": 19, "unit": "mm" },
      { "name": "back_thickness", "value": 5, "unit": "mm" },
      { "name": "x", "value": 0, "unit": "mm" },
      { "name": "y", "value": 0, "unit": "mm" },
      { "name": "z", "value": 0, "unit": "mm" },
      { "name": "cabinet_height", "value": 800, "unit": "mm" },
      { "name": "cabinet_width", "value": 500, "unit": "mm" },
      { "name": "cabinet_depth", "value": 600, "unit": "mm" },
      { "name": "explosion_factor", "value": 50, "unit": "mm" }
    ],
    "parts": {
      "A": {
        "length": "${cabinet_depth}",
        "width": "${cabinet_width}",
        "thickness": "${thickness}"
      },
      "B": {
        "length": "${cabinet_height}",
        "width": "${cabinet_depth}",
        "thickness": "${thickness}"
      },
      "C": {
        "length": "${cabinet_height}",
        "width": "${cabinet_width} - (2 * ${thickness})",
        "thickness": "${back_thickness}"
      }
    },
    "assembly": [
      {
        "id": "unterboden",
        "part": "A",
        "position": ["${x}", "${y} - ${explosion_factor}", "${z}"],
        "rotation": [90, 0, 90]
      },
      {
        "id": "oberboden",
        "part": "A",
        "position": ["${x}", "${y} + ${cabinet_height} + ${thickness} + ${explosion_factor}", "${z}"],
        "rotation": [90, 0, 90]
      },
      {
        "id": "seite_links",
        "part": "B",
        "position": ["${x} - (${cabinet_width} / 2) + (${thickness} / 2) - ${explosion_factor}", "${y} + (${cabinet_height} / 2) + (${thickness} / 2)", "${z}"],
        "rotation": [0, 90, 90]
      },
      {
        "id": "seite_rechts",
        "part": "B",
        "position": ["${x} + (${cabinet_width} / 2) - (${thickness} / 2) + ${explosion_factor}", "${y} + (${cabinet_height} / 2) + (${thickness} / 2)", "${z}"],
        "rotation": [0, 90, 90]
      },
      {
        "id": "r√ºckwand",
        "part": "C",
        "position": ["${x}", "${y} + (${cabinet_height} / 2) + (${thickness} / 2)", "${z} - (${cabinet_depth} / 2) + (${back_thickness} / 2) - ${explosion_factor}"],
        "rotation": [0, 0, 90]
      }
    ]
  }
  